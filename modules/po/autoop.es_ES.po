msgid "Hostmasks(s) Removed from user {1}"
msgstr "Máscara(s) borrada(s) del usuario {1}"

#: autoop.cpp:478
msgid "User {1} removed"
msgstr "Usuario {1} eliminado"

#: autoop.cpp:484
msgid "That user already exists"
msgstr "Ese usuario ya existe"

